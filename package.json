{
  "name": "geogrid-trainer",
  "version": "1.0.0",
  "description": "World map application with GeoGrid category data for geography practice",
  "type": "module",
  "scripts": {
    "scrape": "node scripts/scrape-categories.js",
    "download": "node scripts/download-raw-data.js",
    "download:wikipedia": "node scripts/download-raw-data.js --wikipedia-only",
    "download:external": "node scripts/download-raw-data.js --external-only",
    "download:countries": "node scripts/download-raw-data.js --countries-only",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "playwright": "^1.40.0"
  },
  "devDependencies": {
    "typescript": "^5.3.0",
    "vitest": "^1.0.0",
    "zod": "^3.22.0"
  }
}
